{
    "name": "Memotion Web",
    "dockerComposeFile": "../../docker-compose.yaml",
    "service": "memotion_web",
    "workspaceFolder": "/usr/src/frontend",
    "settings": {
        "cSpell.workspaceRootPath": "${workspaceFolder}/../.vscode",
    },
    "extensions": [
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "gruntfuggly.todo-tree",
        "mhutchie.git-graph",
        "ryu1kn.partial-diff",
        "streetsidesoftware.code-spell-checker",
    ]
}
